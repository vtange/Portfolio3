<div class="gallery-container">
	<!-- if projects -->
	{{#if projects}}
		{{#each projects}}
		<!--<h2>{{name}}</h2>-->
				{{#if mainImage}}
				<a href="{{link}}"><div class="col-sm-4 col-md-4 gallery-image" style="background-image:url({{cloudinaryUrl mainImage}})">
				</div></a>
				{{/if}}
		{{/each}}
	{{else}}
	<!-- else -->
	<h3 class="text-muted">There are no image galleries yet.</h3>
	{{/if}}
</div>
